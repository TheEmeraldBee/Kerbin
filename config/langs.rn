use crate::EditorCommand;

pub fn load(api) {
    // Rust
    api.commands.add(EditorCommand::RegisterLanguageExt("rs", "rust"));

    // Python
    api.commands.add(EditorCommand::RegisterLanguageExt("py", "python"));

    // Rune
    api.commands.add(EditorCommand::RegisterLanguageExt("rn", "rune"));
}
